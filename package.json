{
  "name": "@dulliag/components",
  "version": "0.7.1",
  "author": "tklein1801",
  "description": "ReactJS component libary",
  "keywords": [],
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/components.esm.js",
  "homepage": "https://dulliag.de",
  "repository": {
    "type": "git",
    "url": "https://github.com/DulliAG/components.git"
  },
  "bugs": {
    "url": "https://github.com/DulliAG/components/issues"
  },
  "files": [
    "dist",
    "src",
    "style"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/components.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/components.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.8.4",
