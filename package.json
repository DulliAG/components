    "react": ">=18",
    "react-dom": ">=18",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.1",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "husky": "^6.0.0",
    "size-limit": "^5.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.7.3"
  },
  "peerDependencies": {
    "@types/react": ">=18",
